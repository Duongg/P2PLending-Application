<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="duongdd.se06000.p2plendingapplication">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:label="P2P Lending"
        android:theme="@style/AppTheme">
        <activity android:name=".PaymentDetailActivity"></activity>
        <activity android:name=".UpdateInformationUserActivity" />
        <activity android:name=".RegisterActivity" />
        <activity android:name=".DetailInvestmentCallingActivity" />
        <activity android:name=".DetailInvestedCompanyActivity" />
        <activity android:name=".DetailCompanyActivity" />
        <activity android:name=".InvestedDetailsActivity" />
        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar" />
    </application>

</manifest>